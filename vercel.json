{
  "installCommand": "python -m pip install --upgrade pip && python -m pip install -r requirements.txt",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "crons": [
    {
      "path": "/api/internal/rollcall-tick",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/internal/rollcall-expire",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
