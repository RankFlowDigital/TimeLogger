{
  "builds": [
    {
      "src": "Dockerfile",
      "use": "@vercel/docker"
    }
  ],
  "crons": [
    {
      "path": "/api/internal/rollcall-tick",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/internal/rollcall-expire",
      "schedule": "*/5 * * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
